{
  "name": "<%= slugify(projectName) %>",
  "version": "<%= version %>",
  "dependencies": {<% var depn = [];
      if (includeJquery) {
        depn.push('\n    "jquery": "*"');
      }
      if (includeZepto) {
        depn.push('\n    "jqueryZepto": "*"');
      }
      if (includeBootstrap) {
        depn.push('\n    "bootstrap": "*"');
      }
    %><%- depn.join(',') %>
  }
}
